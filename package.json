{
  "name": "forking-store",
  "version": "2.2.0",
  "description": "",
  "type": "module",
  "exports": {
    "import": "./dist/esm/forking-store.js",
    "require": "./dist/cjs/forking-store.cjs"
  },
  "scripts": {
    "build": "concurrently --group --prefix-colors auto \"npm:build:cjs\" \"npm:build:esm\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc",
    "lint": "concurrently --group --prefix-colors auto \"npm:lint:*(!fix)\"",
    "lint:fix": "concurrently --group --prefix-colors auto \"npm:lint:*:fix\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --cache --fix",
    "lint:format": "prettier --cache --check .",
    "lint:format:fix": "prettier --cache --write .",
    "prepare-todo, doesn't work because of type errors": "npm run build",
    "release": "release-it",
    "test": "node --test"
  },
  "author": "Sergey Andreev",
  "license": "MIT",
  "dependencies": {
    "rdflib": "^2.2.19"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "eslint": "^8.19.0",
    "eslint-plugin-n": "^16.1.0",
    "prettier": "^3.0.3",
    "release-it": "^16.1.5",
    "typescript": "^5.7.3"
  },
  "repository": {
    "url": "https://github.com/redpencilio/forking-store.git"
  },
  "release-it": {
    "npm": {
      "publish": false
    }
  }
}
